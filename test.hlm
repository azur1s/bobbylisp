-- Source

fun make_add() : (num, num) -> num =
    \a, b -> num = {
        println(a + b);
        return a + b;
    };

let add = make_add();

fun main() = {
    let foo = 34;

    let bar = 35 in {
        let r = add(foo, bar);

        match r
        | 69 -> println(r);
        | 42 -> println("What");
        | _  -> println("Unreachable");
    };
};

